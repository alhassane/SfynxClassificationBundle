{
  "name": "sfynx-project/sonata-classification-bundle",
  "type": "symfony-bundle",
  "description": "This Bundle overload the SonataClassificationBundle",
  "keywords": ["cmf", "sonata"],
  "homepage": "http://pigroupe.github.io/cmf-sfynx/",
  "license": "MIT",
  "authors": [
    {
      "name": "Etienne de Longeaux",
      "email": "etienne.delongeaux@gmail.com"
    },
    {
      "name": "Sfynx Community",
      "homepage": "https://github.com/pigroupe/SfynxClassificationBundle/contributors"
    }
  ],

  "require": {
    "symfony/framework-bundle": "~2.8"
  },

  "require-dev": {
    "symfony/phpunit-bridge": "~2.7|~3.0",
    "phpunit/phpunit": "*"
  },

  "suggest": {
  },

  "conflict": {
  },

  "autoload": {
    "psr-0": { "Sfynx\\ClassificationBundle": "" }
